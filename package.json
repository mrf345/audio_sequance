{
  "name": "audio_sequence",
  "version": "0.0.14",
  "description": "simple module to control, monitor and play multiple audio files in sequences at a time.",
  "main": "lib/index.js",
  "scripts": {
    "test": "standard ./src/* && jest ./src",
    "build": "./pages.sh",
    "format": "standard --fix ./src/*",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "push": "babel --config-file ./babel.config.js src/ --out-dir lib/ && npm version patch && npm login && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrf345/audio_sequance.git"
  },
  "keywords": [
    "audio",
    "sequence",
    "playlist",
    "repeater"
  ],
  "author": "Mohamed Feddad",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mrf345/audio_sequance/issues"
  },
  "homepage": "https://github.com/mrf345/audio_sequance#readme",
  "dependencies": {
    "jsdom": "^15.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@types/jest": "^24.9.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "coveralls": "^3.1.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "standard": "^14.3.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "standard": {
    "globals": [
      "beforeEach",
      "afterEach",
      "it",
      "test",
      "assert",
      "window",
      "fetch",
      "document",
      "expect",
      "describe",
      "jest"
    ]
  },
  "browser": {
    "jsdom": false,
    "https": false,
    "fs": false
  }
}
